
public class _0002_ClimbingStairs2 {

	 
	public static void main(String[] args) {
		
		int n = 4;
		 
		int result = solve(n);
		System.out.println(result);
		
	}

	private static int solve(int n) {
		
		 int[] dp = new int[n + 1];
		 
		 dp[0] = 1;
		 dp[1] = 1;
		 
		 
		 
		 return dp[n];
	}
}
