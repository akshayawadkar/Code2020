import java.util.Arrays;

public class Test {

	public static void main(String[] args) {

		int n = 4;

		int result = solve(n);

		System.out.println(result);
	}

	private static int solve(int n) {

		int[] zdp = new int[n + 1];
		int[] odp = new int[n + 1];

		zdp[1] = 1;
		odp[1] = 1;

		for (int i = 2; i <= n; i++) {
			zdp[i] = odp[i - 1];
			odp[i] = zdp[i - 1] + odp[i - 1];
		}
		
		System.out.println(Arrays.toString(zdp));
		System.out.println(Arrays.toString(odp));
		

		return zdp[n] + odp[n];
	}

}