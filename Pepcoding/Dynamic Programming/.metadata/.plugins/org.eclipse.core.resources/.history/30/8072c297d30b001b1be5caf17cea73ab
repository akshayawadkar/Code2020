
public class _007_MinCostPaths {

	public static void main(String[] args) {
		
		int[][] arr = {{0,1,4,2,8,2},
					   {4,3,6,5,0,4},
					   {1,2,4,1,4,6},
					   {2,0,7,3,2,2},
					   {3,1,5,9,2,4},
					   {2,7,0,8,5,1}};
		
		int minCost = solve(arr, arr.length - 1, arr[0].length - 1);
		
	}
}
