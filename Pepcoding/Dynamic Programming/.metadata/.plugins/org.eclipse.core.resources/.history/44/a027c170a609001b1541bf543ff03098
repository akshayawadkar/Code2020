import java.util.ArrayList;

public class _8_PrintSubsetsWithSum {

	private static int solve(int[] arr, int sum, int n, String curr) {

		if(sum == 0) {
			System.out.println(curr);
			return 1;
		}
		
		if(n < 0) {
			return 0;
		}
		
		if(arr[n] <= sum) {
			return solve(arr, sum, n - 1, curr) + solve(arr, sum - arr[n], n - 1, curr + arr[n] +" ");
		}else {
			return solve(arr, sum, n - 1, curr);
		}
		 
	}

	 
	public static void main(String[] args) {

		int[] arr = { 1, 2, 3, 3 };
		int sum = 6;
		 
		int result = solve(arr, sum, arr.length - 1, "");
		System.out.println(result);
	}
}