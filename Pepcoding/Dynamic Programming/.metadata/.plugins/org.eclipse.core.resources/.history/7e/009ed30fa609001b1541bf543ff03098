import java.util.*;

public class _8_PrintSubsetsWithSum {

	private static int solve(int[] arr, int sum, int n) {

		if(sum == 0) {
			return 1;
		}
		
		if(n < 0) {
			return 0;
		}
		
		if(arr[n] <= sum) {
			result.add(arr[n]);
			int tmp =  solve(arr, sum, n - 1) + solve(arr, sum - arr[n], n - 1);
			result.remove(result.size() - 1);
			return tmp;
		}else {
			return solve(arr, sum, n - 1);
		}
		 
	}

	static List<Integer> result;
	public static void main(String[] args) {

		int[] arr = { 1, 2, 3, 3 };
		int sum = 6;
		result = new ArrayList<>();
		int result = solve(arr, sum, arr.length - 1);
		System.out.println(result);
	}
}