
public class _0003_ClimbingStairsWithJumps2 {
	
	
	public static void main(String[] args) {
		
		int[] arr = {3, 3, 0, 2, 2, 3};
		int n = arr.length;
		
		int result = solve(arr, 0, n);
		System.out.println(result);
		
	}

	private static int solve(int[] arr, int idx, int n) {
		
		int[] dp = new int[n + 1];
		
		
		return dp[n];
		 
	}

}
