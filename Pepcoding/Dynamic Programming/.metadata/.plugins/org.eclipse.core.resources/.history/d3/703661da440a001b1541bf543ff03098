import java.util.Arrays;

public class _0001_LIS {

	public static void main(String[] args) {
		
		int[] arr = {10, 22, 9, 33, 21, 50, 41, 60, 80, 3};
		int LIS  = solve(arr);
		System.out.println(LIS);
		
	}

	private static int solve(int[] arr) {
		
		int n = arr.length;
		int[] dp = new int[n];
		
		Arrays.fill(dp, 1);
		
		
	
		
		return 0;
	}
}
