
public class _0008_DispAnArray {
	
	public static void main(String[] args) {
		
		int[] arr = {1,2,3,4,5};
		int n = arr.length - 1;
		solve(arr, n);
		solveReverse(arr, n);
		
		
	}

	private static void solve(int[] arr, int n) {
		
		if(n == -1) {
			return;
		}
		solve(arr, n - 1);
		System.out.println(arr[n]);
		
	}

}
