package basics;

public class _006_2SCompliment {

	public static void main(String[] args) {

		String ip = "10110001";

		int num = Integer.parseInt(ip, 2);

		System.out.println(num);
		System.out.println(ip);

		// 2's Compliment
		
		int inverted = ~num;
		
		int twosCompli = inverted + 1;
		System.out.println(twosCompli);
		System.out.println(Integer.toBinaryString(twosCompli));
		
		

	}

	 

}
